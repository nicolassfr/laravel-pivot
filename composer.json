{
    "name": "nicolassfr/laravel-pivot",
    "description": "Custom package of fico7489/laravel-pivot adding pivotSyncing and pivotSynced methods to self fire sync() Laravel method.",
    "keywords": [
        "laravel BelongsToMany events",
        "eloquent extra events",
        "laravel pivot events",
        "laravel sync events",
        "eloquent events"
    ],
    "homepage": "https://github.com/nicolassfr/laravel-pivot",
    "support": {
        "issues": "https://github.com/nicolassfr/laravel-pivot/issues",
        "source": "https://github.com/nicolassfr/laravel-pivot"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Nicolas Samat",
            "email": "bonjour@nicolass.fr",
            "homepage": "http://nicolass.fr",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/database": ">5.5.0  || 6.*"
    },
    "require-dev": {
        "orchestra/testbench": ">3.0",
        "friendsofphp/php-cs-fixer": ">2.0"
    },
    "autoload": {
        "psr-4": {
            "Nicolassfr\\Laravel\\Pivot\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nicolassfr\\Laravel\\Pivot\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
